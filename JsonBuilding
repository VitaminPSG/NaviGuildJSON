public class JsonBuilding {
    //Координаты здания [Latitude][Longitude]
    private double[] point;
    //Номер здания
    private String num;

    public double[] getPoint() {
        return point;
    }

    public void setPoint(double[] point) {
        this.point = point;
    }

    public String getNum() {
        return num;
    }

    public void setNum(String num) {
        this.num = num;
    }
}
